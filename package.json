{
  "name": "spectre",
  "version": "1.0.0",
  "private": true,
  "license": "MPL-2.0",
  "author": "r-priyam <roy_priyam@outlook.com>",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch --onSuccess \"node --enable-source-maps dist/index.js\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src --ext ts --fix",
    "start": "NODE_ENV=production node --enable-source-maps dist/index.js",
    "typecheck": "tsc -b src",
    "watch": "tsup --watch"
  },
  "prettier": "@sapphire/prettier-config",
  "eslintConfig": {
    "extends": "@sapphire"
  },
  "dependencies": {
    "@discordjs/collection": "^0.5.0",
    "@sapphire/decorators": "^4.3.3",
    "@sapphire/discord-utilities": "^2.10.2",
    "@sapphire/discord.js-utilities": "^4.9.3",
    "@sapphire/fetch": "^2.3.0",
    "@sapphire/framework": "next",
    "@sapphire/plugin-api": "^3.2.0",
    "@sapphire/plugin-editable-commands": "^1.2.0",
    "@sapphire/plugin-logger": "^2.2.0",
    "@sapphire/plugin-subcommands": "^2.2.0",
    "@sapphire/time-utilities": "^1.7.3",
    "@sapphire/type": "^2.2.0",
    "@sapphire/utilities": "^3.6.1",
    "discord-api-types": "^0.30.0",
    "discord.js": "^13.6.0",
    "dotenv-cra": "^3.0.2",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@sapphire/eslint-config": "^4.3.2",
    "@sapphire/prettier-config": "^1.4.1",
    "@sapphire/ts-config": "^3.3.3",
    "@types/eslint": "^8",
    "@types/node": "^17.0.0",
    "@types/ws": "^8.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.1",
    "tsup": "^5.12.1",
    "typescript": "^4.6.3"
  },
  "imports": {
    "#root/*": "./dist/*.js",
    "#lib/*": "./dist/lib/*.js",
    "#utils/*": "./dist/lib/utils/*.js"
  },
  "packageManager": "yarn@3.2.0"
}
