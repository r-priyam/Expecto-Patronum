{
  "name": "spectre",
  "version": "1.0.0",
  "private": true,
  "license": "Mozilla Public License 2.0",
  "author": "r-priyam <roy_priyam@outlook.com>",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch --onSuccess \"yarn start\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src --ext ts --fix",
    "start": "node --enable-source-maps dist/index.js",
    "typecheck": "tsc -b src",
    "watch": "tsup --watch"
  },
  "prettier": "@sapphire/prettier-config",
  "eslintConfig": {
    "extends": "@sapphire"
  },
  "dependencies": {
    "@discordjs/collection": "^0.3.2",
    "@sapphire/decorators": "^3.1.4",
    "@sapphire/discord-utilities": "^2.2.3",
    "@sapphire/discord.js-utilities": "^4.1.3",
    "@sapphire/fetch": "^2.0.3",
    "@sapphire/framework": "next",
    "@sapphire/plugin-api": "^3.1.0",
    "@sapphire/plugin-editable-commands": "^1.1.1",
    "@sapphire/plugin-logger": "^2.1.0",
    "@sapphire/plugin-subcommands": "^2.1.0",
    "@sapphire/time-utilities": "^1.4.9",
    "@sapphire/type": "^2.1.2",
    "@sapphire/utilities": "^3.1.0",
    "discord-api-types": "^0.30.0",
    "discord.js": "^13.3.1",
    "dotenv-cra": "^3.0.2",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@sapphire/eslint-config": "^4.3.2",
    "@sapphire/prettier-config": "^1.2.6",
    "@sapphire/ts-config": "^3.1.6",
    "@types/eslint": "^8",
    "@types/node": "^17.0.0",
    "@types/ws": "^8.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.0",
    "tsup": "^5.11.11",
    "typescript": "^4.5.2"
  },
  "imports": {
    "#root/*": "./dist/*.js",
    "#lib/*": "./dist/lib/*.js",
    "#utils/*": "./dist/lib/util/*.js"
  },
  "packageManager": "yarn@3.2.0"
}
